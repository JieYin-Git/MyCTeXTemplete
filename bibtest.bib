@phdthesis{sun,
	type = {博士},
	title = {模型未知下试验设计的构造},
	abstract = {试验设计是统计学的重要分支之一,它不仅在理论上有重要意义,在实际领域也具有重大的应用价值。古典的试验设计方法,如正交设计、回归设计、区组设计、拉丁方设计、响应曲面设计等,已建立了丰富的理论。它们均假定模型的形式已知,但其中含有未知参数,要通过试验来估计其中的未知参数,并要求在一定的意义下,试验设计达到最优。这种设计我们称为模型已知下的试验设计。随着新技术和科学的飞速发展,人类面对的问题越来越复杂,需要突破古典试验设计强烈依赖于模型的限制；另外,随着计算机技术的发展,许多试验的前期研究,可以在计算机上进行,这可大大节省试验开支,又可显著加快研究进程。在计算机试验中模型不存在随机试验误差,与模型未...},
	language = {英文;},
	school = {南开大学},
	author = {孙, 法省},
	year = {2010},
	keywords = {别名矩阵, 差阵, 超饱和设计, 导出矩阵, 等距设计, 分片空间填充设计, 广义字长型, 广义最小低阶混杂, 计算机试验, 伽罗瓦域, 均匀性, 拉丁超立方体设计, 偏差, 嵌套差阵, 嵌套空间填充设计, 示性函数, 相遇数, 正交表, 正交列设计, Alias matrix, Coincidence number, Computer experiment, Difference matrix, Discrepancy, Equidistant design, Galois field, Generalized minimum aberration, Generalized wordlength pattern, Indicator function, Induced matrix, Latin hypercube design, Nested difference matrix, Nested space-filling design, Orthogonal array, Orthogonal column design, Sliced space-filling design, Supersaturated design, Uniformity},
}

@article{zhang__2019,
	title = {一类相依网络的动态聚类分析},
	volume = {35},
	issn = {1001-4268},
	abstract = {动态复杂网络因广泛应用于群体生态学、社会生态学、生物学和因特网等领域而成为研究的热门问题之一,聚类分析是提取网络结构的常用工具.以往关于网络聚类的文献大多基于某种特定的条件独立假设.本文我们结合了随机区组模型,隐马尔科夫链和自回归模型给出了一个新模型来放宽这个假设,并给出了相应的统计推断和VEM算法.蒙特卡洛模拟表现良好,表明了我们的方法的一致性和稳健性.},
	language = {中文;},
	number = {04},
	journal = {应用概率统计},
	author = {张, 齐 and 李, 新民 and 王, 维维 and 王, 亮},
	year = {2019},
	keywords = {变分近似方法, 动态相依网络, 蒙特卡洛模拟, 随机区组-马氏自回归模型, dependent network sequences, Monte Carlo simulation, SMBs-HMM-AR model, variation approximation method},
	pages = {397--407},
}



@article{__2007,
	title = {因子平方和的正交对照分解及其应用},
	abstract = {本文把a水平因子的平方和分解成相互正交的a-1个对照的平方和,这样总变差平方和就可以分解成a个部分(包括残差项)。然后又将该分解方法推广到了多因子的情形,并通过因子平方和的分解找到了多因子交互效应对应的对照向,这使得多水平因子交互效应的计算和解释更加容易,也为方差分析带来了更多的方便.最后给出了几个应用示例。},
	language = {中文;},
	journal = {2007均匀试验设计学术交流会论文集},
	author = {孙, 法省 and 刘, 民千},
	year = {2007},
	note = {Place: 中国北京
	Publisher: 中国数学会均匀设计分会},
	keywords = {方差分析, 因子平方和, 正交表, 正交对照, 总变差平方和},
	pages = {130--142},
}


